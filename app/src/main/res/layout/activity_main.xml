            android:layout_height="wrap_content"
            android:text="6"
            android:layout_marginHorizontal="65dp"
            android:textColor="@color/midnight_blue"
            android:textSize="23dp"
            />
        <CheckBox
            android:id="@+id/enr16"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
